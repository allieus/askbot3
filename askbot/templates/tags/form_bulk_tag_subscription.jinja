{% extends "one_column_body.jinja" %}
{% import "macros.jinja" as macros %}
<!-- template form_bulk_tag_subscription.jinja -->
{% block title %}{% filter trim %}{{action}} {% trans %}Tag subscriptions{% endtrans %}{% endfilter %}{% endblock %}
{% block content %}
<h1 class="section-title">{{action}} {% trans %}Tag Subscriptions{% endtrans %}</h1>
<form action="." method="POST" accept-charset="utf-8">
<table border="0">
    {{form.as_table()}}
    <tr><td/><td><input type="submit" value="{% trans %}Save{% endtrans %}"></td></tr>
</table>
</form>
{% endblock %}
{% block endjs %}
<script type="text/javascript" charset="utf-8">
    {{ macros.tag_autocomplete_js() }}
</script>
{% endblock %}
<!-- end template form_bulk_tag_subscription.jinja -->
